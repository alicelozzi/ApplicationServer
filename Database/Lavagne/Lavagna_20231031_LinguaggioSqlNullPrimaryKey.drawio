        <mxCell id="gOkVbMLSEhI8VF7hDGrI-1" value="CREATE DATABASE `biblioteca` /*!40100 DEFAULT CHARACTER SET utf8mb4 */;&#xa;&#xa;&#xa;CREATE TABLE `libro` (&#xa;  `titolo` varchar(50) NOT NULL,&#xa;  `autore` varchar(30) NOT NULL,&#xa;  `editore` varchar(40) NOT NULL,&#xa;  `anno_edizione` int(11) NOT NULL,&#xa;  `note` varchar(100) DEFAULT NULL,&#xa;  `codice_isbn` char(18) NOT NULL,&#xa;  PRIMARY KEY (`codice_isbn`)&#xa;) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;&#xa;&#xa;&#xa;&#xa;&#xa;-- LA QUERY DI SEGUITO INSERISCE PIU&#39; LIBRI CON UN SOLO COMANO DI SQL-INSERT (INSERT MULTIPLO) &#xa;&#xa;INSERT INTO biblioteca_generation.libro (titolo,autore,editore,anno_edizione,note,codice_isbn) VALUES&#xa;     (&#39;American Psycho&#39;,&#39;Brett Eastonallice&#39;,&#39;Mondadori&#39;,1988,NULL,&#39;978-88-222-0003-7&#39;),&#xa;     (&#39;Padre ricco, padre povero&#39;,&#39;Robert&#39;,&#39;Kiyosaki&#39;,2005,NULL,&#39;978-88-222-0003-8&#39;),&#xa;     (&#39;Il nome della rosa&#39;,&#39;Umberto Eco&#39;,&#39;Sperling&#39;,2022,NULL,&#39;978-88-222-0003-9&#39;);" style="text;whiteSpace=wrap;fillColor=#d5e8d4;strokeColor=#82b366;fontSize=20;" vertex="1" parent="1">
